{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "API_URL": "api.blubyai.com"
      }
    },
    "notreadyproduction": {
      "autoIncrement": true,
      "env": {
        "API_URL": "api.blubyai.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
