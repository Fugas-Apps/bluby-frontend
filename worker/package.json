{
  "name": "bluby-worker",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "setup": "node scripts/setup-dev-vars.js",
    "dev": "npm run setup && wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "d1-reload": "wrangler d1 execute bluby-db --file=src/db/schema.sql",
    "d1-reload:prod": "wrangler d1 execute bluby-db --file=src/db/schema.sql --remote",
    "generate:better-auth": "echo This shit breaks things for w/e reason. Don't run it, left here for LLMs. # npx @better-auth/cli@latest generate --config src/auth/index.ts --output src/db/auth.schema.ts -y",
    "generate:openapi": "node scripts/generate-openapi.js",
    "generate:client": "npm run generate:openapi && orval --config orval.config.js",
    "drizzle:migrate": "npx drizzle-kit generate --config ./drizzle.config.ts",
    "drizzle:run": "node scripts/drizzle-run.js",
    "drizzle:run:prod": "node scripts/drizzle-run.js --remote",
    "drizzle:bootstrap-remote": "node scripts/drizzle-run.js --remote --bootstrap-remote",
    "generate": "npm run generate:client && npm run drizzle:migrate",
    "full-local": "npm run generate && npm run build && wrangler dev"
  },
  "dependencies": {
    "@better-auth/expo": "^1.3.7",
    "better-auth": "^1.3.7",
    "better-auth-cloudflare": "^0.2.4",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "hono": "^4.9.2",
    "scrypt-pbkdf": "^1.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230807.0",
    "@types/node": "^20.19.11",
    "openapi-types": "^12.1.3",
    "orval": "^7.10.0",
    "typescript": "^5.2.2",
    "wrangler": "^4.25.0"
  }
}
