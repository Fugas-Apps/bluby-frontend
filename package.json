{
  "name": "my-expo-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start",
    "dev": "API_URL=http://localhost:8787 expo start",
    "prod": "API_URL=https://api.blubyai.com expo start",
    "android:dev": "API_URL=http://localhost:8787 expo start --android",
    "android:prod": "API_URL=https://api.blubyai.com expo start --android",
    "ios:dev": "API_URL=http://localhost:8787 expo start --ios",
    "ios:prod": "API_URL=https://api.blubyai.com expo start --ios",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "full-local": "bash -c 'trap \"kill 0\" SIGINT SIGTERM; npm --prefix worker run dev & npm run dev & wait'"
  },
  "dependencies": {
    "@better-auth/expo": "^1.3.7",
    "@expo/config-plugins": "~10.1.1",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/prebuild-config": "~9.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.2.8",
    "@tanstack/react-query": "^5.85.5",
    "axios": "^1.11.0",
    "better-auth": "^1.3.7",
    "better-auth-cloudflare": "^0.2.4",
    "expo-constants": "^17.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "^7.1.7",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "^14.2.0",
    "jose": "^6.0.13",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/axios": "^0.9.36",
    "@types/node": "^24.1.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "expo": "53.0.22",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@expo/config-plugins",
          "@expo/metro-runtime",
          "@expo/prebuild-config"
        ]
      }
    }
  },
  "main": "node_modules/expo-router/entry",
  "private": true
}
